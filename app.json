{
  "expo": {
    "name": "Molinillo",
    "slug": "molinillo",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "packagerOpts": {
      "nonPersistent": true
    },
    "version": "2.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/logo.png",
      "backgroundColor": "#4B006E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "android": {
      "versionCode": 5,
      "package": "com.molinillo.molinillomobile"
    },
    "ios": {
      "bundleIdentifier": "com.molinillo.molinillomobile",
      "buildNumber": "2",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Once given permission to access the user's camera, this app allows the user to scan barcodes."
      }
    },
    "description": "Discover and engage with ethically sourced fine chocolate from around the globe.",
    "githubUrl": "https://github.com/jcuanm/molinillo-mobile"
  }
}